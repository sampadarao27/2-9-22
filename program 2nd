#include <stdio.h>
/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/
void max(int ,int ,int ,int );

int main() {
  int a,b,c,d;
  int largenum;
   scanf("%d %d %d %d",&a,&b,&c,&d);
  max(a,b,c,d);

 return 0;
}
void max(int a,int b,int c,int d)
{
    if(a>b && a>c && a>d)
    printf("%d",a);
    else if(b>c && b>d)
    printf("%d",b);
    else if(c>d)
    printf("%d",c);
    else
    printf("%d",d);

}
